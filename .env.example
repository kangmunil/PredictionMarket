# Polymarket Configuration
POLY_HOST="https://clob.polymarket.com"
POLY_CHAIN_ID=137

# !!! SECURITY WARNING !!!
# NEVER commit your actual private key to version control!
# Replace these placeholder values with your actual credentials
PRIVATE_KEY="YOUR_PRIVATE_KEY_HERE"
FUNDER_ADDRESS="YOUR_WALLET_ADDRESS_HERE"

# External APIs
NEWS_API_KEY="YOUR_NEWS_API_KEY_HERE"

# AI Models - OpenRouter (다양한 모델 사용 가능)
# Get your API key from: https://openrouter.ai
OPENROUTER_API_KEY="YOUR_OPENROUTER_API_KEY_HERE"
OPENROUTER_BASE_URL="https://openrouter.ai/api/v1"

# AI Model Selection (OpenRouter 모델명)
# Entity Extraction (빠르고 저렴한 모델) - ~$0.25/M tokens
AI_MODEL_ENTITY="anthropic/claude-3-haiku"
# Market Analysis (강력한 추론 모델) - ~$3/M tokens
AI_MODEL_ANALYSIS="anthropic/claude-3.5-sonnet"
# Embedding (OpenAI embedding 모델) - ~$0.02/M tokens
AI_MODEL_EMBEDDING="openai/text-embedding-3-small"

# (Optional) OpenAI API - Embedding용 또는 fallback
OPENAI_API_KEY="YOUR_OPENAI_API_KEY_HERE"

# Supabase (for RAG System)
# Setup: https://supabase.com/dashboard
# 1. Create project or select existing
# 2. Go to Settings > API
# 3. Copy URL and anon/public key
SUPABASE_URL="YOUR_SUPABASE_URL_HERE"
SUPABASE_KEY="YOUR_SUPABASE_ANON_KEY_HERE"

# Target Wallets for Copy Trading
# Example: distinct-baguette
TARGET_WALLET_1="0xe00740bce98a594e26861838885ab310ec3b548c"
TARGET_WALLET_2=""
TARGET_WALLET_3=""

# Redis Configuration (for Budget Manager)
REDIS_URL="redis://localhost:6379"

# Telegram Notifications
